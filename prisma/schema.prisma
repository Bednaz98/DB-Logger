// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Log {
  id               String   @id @unique
  sessionID        String   @default(uuid())
  displayTimeStamp String   @default("")
  timeStamp        BigInt
  title            String
  tags             String[]
  message          String?
}
