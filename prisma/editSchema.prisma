model General {
  id               String   @id
  displayTimeStamp String   @default("")
  timeStamp        BigInt
  title            String
  tags             String[]
  message          String?
}

model Verbose {
  id               String   @id @default(uuid())
  displayTimeStamp String   @default("")
  timeStamp        BigInt
  title            String
  tags             String[]
  message          String?
}

model Warning {
  id               String   @id
  displayTimeStamp String   @default("")
  timeStamp        BigInt
  title            String
  tags             String[]
  message          String?
}

model Error {
  id               String   @id
  displayTimeStamp String   @default("")
  timeStamp        BigInt
  title            String
  tags             String[]
  message          String?
}
